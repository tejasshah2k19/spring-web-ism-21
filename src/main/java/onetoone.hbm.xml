<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="com.entity.onetoone.StockEntity" table="stocks">
		<id name="stockId">
			<generator class="increment"></generator>
		</id>
		<property name="stockName"></property>
		<property name="sector"></property>

		<one-to-one name="stockDetail" cascade="all"></one-to-one>

	</class>

	<class name="com.entity.onetoone.StockDetailEntity"
		table="stockDetail">
		<id name="stockDetailId">
			<generator class="foreign">
				<param name="property">stock</param>
			</generator>
		</id>
		<property name="price"></property>
		<property name="allTimeHighPrice"></property>
		<property name="allTimeLowPrice"></property>
		<one-to-one name="stock"></one-to-one>

	</class>
</hibernate-mapping>